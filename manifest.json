{
  "manifest_version": 2,
  "name": "Katsudon",
  "version": "1.0",
  "description": "Sends Leetcode solution to mongoDB",
  "icons": {
    "16": "/assets/katsudon_16.png",
    "48": "/assets/katsudon_48.png",
    "128": "/assets/katsudon_128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "/assets/katsudon_16.png",
      "48": "/assets/katsudon_48.png",
      "128": "/assets/katsudon_128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["scripts/leetcode.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["tabs", "https://*/*", "http://*/*"]
}
